<%= render 'shared/header_opaque' %>
<%= render partial: 'shared/metatags', locals: { seo: @seo } %>

<!-- Content -->
<div class="content clearfix">
  <div class="section-block grid-filter-menu right left-on-mobile mt-80">
  	<div class="row no-padding">
  		<div class="column width-12 center">
  			<h3 class="mb-mobile-50">Obra de nuestros artistas</h3>
  		</div>
  	</div>
  </div>
  
  <div class="section-block clearfix no-padding-top">
  	<div class="row">
  		<div class="column width-12 content-inner blog-regular center">
        <% @artworks.each do |artwork| %>
          <%= render partial: 'artworks/blog_style_artwork', locals: { artwork: artwork } %>
        <% end %>
      </div>
    </div>
  </div>
</div>
<!-- Content End -->